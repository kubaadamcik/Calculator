<UserControl x:Class="Kalkulačka.View.UserControls.Tlacitko"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kalkulačka.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid d:Background="White">
		<Button x:Name="btnCislo" d:Content="1" Content="{Binding}" FontSize="24" FontWeight="Bold" Background="Transparent"
				Click="btnCislo_Click"/>
		
	</Grid>
</UserControl>
